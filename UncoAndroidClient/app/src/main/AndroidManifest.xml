<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uncoandroidclient">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:configChanges="locale|orientation"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="screens.EndActivity"></activity>
        <activity android:name="screens.QuestionActivity" />
        <activity android:name="screens.WaitingActivity" />
        <activity android:name="screens.ConfigActivity"
            android:configChanges="locale|orientation"/>
        <activity android:name="screens.RankingActivity" />
        <activity android:name="screens.MainActivity" />
        <activity android:name="screens.RegisterActivity" />
        <activity android:name="screens.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>